                                <a class="dropdown-item" asp-action="AddBug" 
                                   asp-route-projectId="@this.ViewContext.RouteData.Values["projectId"]" asp-route-userStoryId="@userStory.Id">
                                    @WorkItemTypesConstants.Bug
                                </a>
                            </div>
                            <a asp-action="GetUserStory"
                               asp-route-UserStoryId="@userStory.Id"
                               asp-route-projectId="@this.ViewContext.RouteData.Values["projectId"]"
                               class="btn btn-sm btn-info">
                                Edit
                            </a>
                            <form method="post" style="display:inline-block;"
                                  asp-action="DeleteUserStory"
                                  asp-route-UserStoryId="@userStory.Id"
                                  asp-route-projectId="@this.ViewContext.RouteData.Values["projectId"]">
                                <button type="submit"
                                        class="btn btn-sm btn-danger" id="delete">
                                    Delete
                                </button>
                            </form>
                        </td>
                    </tr>
                    @foreach (var task in userStory.Tasks)
